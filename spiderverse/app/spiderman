class Spiderman{
    constructor(name, age, actor, movies, studio){
        this.name = name
        this.age = age
        this.actor = actor
        this.movies = movies
        this.studio = studio
    }

    getInfo(){
        var info = "Hey, I'm " + this.actor + " from " + this.studio + " studio"
        return info
    }
}

module.exports = Spiderman